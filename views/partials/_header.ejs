<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-item nav-link" href="/urls">My URLs</a>
          </li>
          <li class="nav-item active">
            <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
          </li>
        </ul>
      </div>
    </div>
    <div>
      <% if(userId !== false && userId!==undefined && users[userId]) { %>
      <form class="form-inline my-2 my-lg-0" method="post" action="/logout">
        <p class="input-group-text mr-sm-2">
          Logged in as: <%= users[userId].email %>
        </p>
        <input
          class="btn btn-outline-success bg-light my-2 my-sm-0"
          type="submit"
          value="logout"
        />
      </form>
      <% } else { %>
      <ul class="navbar-nav mr-auto">
        <li><a class="nav-item nav-link" href="/login">Login</a></li>
        <li><a class="nav-item nav-link" href="/register">Register</a></li>
      </ul>
      <% } %>
    </div>
  </nav>
</header>
